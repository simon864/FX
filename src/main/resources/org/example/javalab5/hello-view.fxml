<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" prefHeight="200.0" prefWidth="400.0" spacing="20.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.example.javalab5.HelloController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
    <children>
        <HBox alignment="CENTER" spacing="10.0">
            <children>
                <TextField fx:id="textField1" promptText="Введите текст здесь" />
                <Button fx:id="switchButton" mnemonicParsing="false" text="-&gt;" style="-fx-pref-width: 100px; -fx-pref-height: 60px; -fx-font-size: 14px;" />
                <TextField fx:id="textField2" promptText="Здесь появится текст" />
            </children>
        </HBox>
    </children>
</VBox>